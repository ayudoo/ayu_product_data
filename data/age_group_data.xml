<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">

    <record forcecreate="True" id="age_group_newborn" model="ayu_product_data.age_group">
      <field name="sequence">5</field>
      <field name="identifier">newborn</field>
      <field name="name">newborn</field>
      <field name="description">Up to 3 months old. Newborn sizes are often identified by the age range in months (0-3) or just 'newborn'.</field>
    </record>

    <record forcecreate="True" id="age_group_infant" model="ayu_product_data.age_group">
      <field name="sequence">4</field>
      <field name="identifier">infant</field>
      <field name="name">infant</field>
      <field name="description">3-12 months old. Infant sizes are often identified by the age range in months (3-12).</field>
    </record>

    <record forcecreate="True" id="age_group_toddler" model="ayu_product_data.age_group">
      <field name="sequence">3</field>
      <field name="identifier">toddler</field>
      <field name="name">toddler</field>
      <field name="description">1-5 years old. Toddler sizes are often identified by the age range in months (12-24) or years (1-5).</field>
    </record>

    <record forcecreate="True" id="age_group_kids" model="ayu_product_data.age_group">
      <field name="sequence">2</field>
      <field name="identifier">kids</field>
      <field name="name">kids</field>
      <field name="description">5-13 years old. All sizes within this age group have been manufactured to fit a child in that age range.</field>
    </record>

    <record forcecreate="True" id="age_group_adult" model="ayu_product_data.age_group">
      <field name="sequence">1</field>
      <field name="identifier">adult</field>
      <field name="name">adult</field>
      <field name="description">Typically teens or older. All sizes within this age group have been manufactured to fit an adult or teen.</field>
    </record>

  </data>
</odoo>
